{
  "entry": [
    "src/index.tsx"
  ],
  "extensions": [
    ".ts",
    ".js",
    ".tsx"
  ],
  "ignorePatterns": [
    "**/node_modules/**",
    "__mocks__/**",
    "bin/**",
    "dist/**",
    "coverage/**",
    "**/*.test.{js,jsx,ts,tsx}",
    "**/*.d.ts"
  ],
  "ignoreUnimported": [
    "src/common/index.ts",
    "src/components/Nodes/BasicNode/BasicNode.tsx",
    "src/index.tsx",
    "src/shared/auto-order-json-translations.js",
    "src/shared/config.ts"
  ],
  "ignoreUnresolved": [],
  "ignoreUnused": [
    "@nivo/core",
    "@nivo/tooltip",
    "i",
    "react-flow-renderer"
  ]
}