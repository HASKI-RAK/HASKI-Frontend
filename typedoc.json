{
  // User friendly name
  "name": "HASKI-Frontend Documentation",
  "excludeExternals": true,
  "excludeNotDocumented": true,
  // "typedoc-plugin-merge-modules",
  "plugin": ["typedoc-plugin-markdown", "typedoc-github-wiki-theme"],
  "theme": "github-wiki",
  "entryPoints": ["./src/index.tsx", "./src/common/core/index.ts"],
  "out": "docs",
  "sourceLinkTemplate": "https://github.com/HASKI-RAK/HASKI-Frontend/blob/main/{path}#L{line}",
  "readme": "README.md",
  "cleanOutputDir": false
  // "mergeModulesMergeMode": "module-category"
}
